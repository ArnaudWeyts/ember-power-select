{
  "name": "ember-power-select",
  "version": "7.2.0",
  "description": "The extensible select component built for ember",
  "keywords": [
    "ember-addon",
    "select",
    "select2",
    "selectize",
    "dropdown",
    "ember-power"
  ],
  "homepage": "http://www.ember-power-select.com",
  "repository": {
    "type": "git",
    "url": "http://github.com/cibernox/ember-power-select.git"
  },
  "license": "MIT",
  "author": {
    "name": "Miguel Camba",
    "email": "miguel.camba@gmail.com"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "@babel/core": "^7.23.2",
    "@ember/render-modifiers": "^2.1.0",
    "@ember/string": "^3.1.1",
    "@embroider/util": "^1.11.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "ember-assign-helper": "^0.4.0",
    "ember-auto-import": "^2.6.3",
    "ember-basic-dropdown": "^7.3.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-typescript": "^5.0.0",
    "ember-concurrency": "^2.0.0 || ^3.0.0",
    "ember-text-measurer": "^0.6.0",
    "ember-truth-helpers": "^3.1.0 || ^4.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-proposal-decorators": "^7.23.5",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.2.1",
    "@embroider/test-setup": "^3.0.3",
    "@glint/template": "^1.2.1",
    "@release-it-plugins/lerna-changelog": "^6.0.0",
    "@types/ember": "^4.0.10",
    "@types/ember-data": "^4.4.15",
    "@types/qunit": "^2.19.9",
    "@types/rsvp": "^4.0.8",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.2",
    "ember-cli": "~5.4.1",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-mirage": "^3.0.2",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-terser": "^4.0.2",
    "ember-code-snippet": "git+https://git@github.com/ef4/ember-code-snippet.git#d054b697098ad52481c94a952ccf8d89ba1f25fe",
    "ember-data": "~5.3.0",
    "ember-href-to": "^5.0.0",
    "ember-load-initializers": "^2.1.2",
    "ember-prism": "^0.13.0",
    "ember-qunit": "^8.0.1",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.4.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.13.0",
    "ember-try": "^3.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-n": "^16.3.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-qunit": "^8.0.1",
    "loader.js": "^4.7.0",
    "memory-scroll": "^1.0.2",
    "miragejs": "^0.1.48",
    "mocha": "^8.0.0",
    "prettier": "^3.1.0",
    "qunit": "^2.20.0",
    "qunit-dom": "^3.0.0",
    "release-it": "^16.0.0",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.1.0",
    "testdouble": "^3.20.1",
    "typescript": "~4.9.5",
    "webpack": "^5.89.0"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0 || >=5.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "overrides": {
    "ember-source": "$ember-source"
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "16.19.0"
  }
}
